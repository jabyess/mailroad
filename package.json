{
  "name": "emailbuilder",
  "version": "0.1.0",
  "description": "React, slate, mjml to make drafting emails better.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jabyess/mailroad.git"
  },
  "scripts": {
    "test": "npm run client-tests && npm run server-tests",
    "client-tests": "webpack --progress --config ./config/webpack.config.dev.js && jest --verbose",
    "server-tests": "mocha",
    "babel-node": "babel-node --presets=es2015",
    "sync-views": "node server/couchdb-views/views.js sync",
    "build": "webpack --progress --debug --config ./config/webpack.config.prod.js && babel ./server/ -d ./dist",
    "develop": "nodemon --inspect server/lib/index.js",
    "webpack": "webpack-dev-server --config ./config/webpack.config.dev.js --open --inline --progress",
    "restart": "./node_modules/.bin/pm2 startOrGracefulReload config/pm2.json --no-pmx --update-env && ./node_modules/.bin/pm2 flush && ./node_modules/.bin/pm2 dump",
    "start": "./node_modules/.bin/pm2 startOrGracefulReload config/pm2.json --no-pmx --update-env",
    "stop": "./node_modules/.bin/pm2 stop config/pm2.json"
  },
  "author": "jbyess@morningconsult.com",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.7.22",
    "axios": "^0.19.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "connect-gzip-static": "^2.0.1",
    "connect-redis": "^3.3.0",
    "couchdb-tools": "0.0.6",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "handlebars": "^4.0.5",
    "ioredis": "^3.0.0",
    "joi": "^10.5.2",
    "juice": "^4.0.2",
    "lodash.merge": "^4.6.0",
    "mjml": "^3.3.0",
    "moment": "^2.17.1",
    "multer": "^1.2.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pm2": "^2.4.6",
    "prop-types": "^15.5.10",
    "react": "^15.4.1",
    "react-autobind": "^1.0.6",
    "react-clipboard.js": "^1.0.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "react-dropzone": "^3.10.0",
    "react-router": "^3.0.0",
    "redis": "^2.6.5",
    "redis-server": "^1.1.0",
    "sharp": "^0.17.1",
    "shortid": "^2.2.6",
    "slate": "^0.19.20",
    "winston": "^2.3.1",
    "winston-loggly-bulk": "^1.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./node_modules/fbjs"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.23.1",
    "dotenv-webpack": "^1.4.5",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.9.0",
    "fbjs": "^0.8.12",
    "file-loader": "^0.10.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.2",
    "mochawesome": "^2.2.0",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^4.0.0",
    "should": "^11.2.1",
    "should-http": "^0.1.1",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.7",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
